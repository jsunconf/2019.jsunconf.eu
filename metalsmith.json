{
  "metadata": {
    "title": "JSUnconf 2019",
    "description": "The Unconference that started it all."
  },
  "source": "content",
  "destination": "dist",
  "plugins": {
    "metalsmith-metadata": {
      "sponsors": "sponsors.yaml"
    },
    "metalsmith-markdown": {},
    "metalsmith-permalinks": {
      "pattern": ":url"
    },
    "metalsmith-layouts": {
      "pattern": "**/*.html",
      "default": "index.pug",
      "directory": "theme/jade"
    },
    "metalsmith-assets": {
      "source": "./theme/assets",
      "destination": "./"
    },
    "metalsmith-stylus": {
      "paths": ["theme/styles"]
    }
  }
}
